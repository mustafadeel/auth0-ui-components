{
  "name": "@auth0-web-ui-components/core",
  "version": "1.1.0",
  "description": "Core utilities for Auth0 Web UI components",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:ts": "tsup",
    "build": "pnpm run clean && pnpm run build:ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "i18n",
    "auth0",
    "core",
    "utilities"
  ],
  "author": "Auth0",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.6.5",
  "devDependencies": {
    "@types/node": "^22.15.27",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "auth0-myorg-sdk": "file:./dependencies/my-org-sdk/auth0-my-org-0.0.353.tgz",
    "@auth0/myaccount": "file:./dependencies/my-account-sdk/auth0-myaccount-0.0.309.tgz",
    "zod": "^3.25.56"
  }
}
