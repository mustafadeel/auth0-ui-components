{
  "name": "@auth0/web-ui-components-core",
  "version": "1.0.0-beta.7",
  "description": "Core utilities for Auth0 Web UI components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "build:ts": "tsup",
    "build": "pnpm clean && pnpm type-check && pnpm build:ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "i18n",
    "auth0",
    "core",
    "utilities"
  ],
  "author": "Auth0",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.6.5",
  "devDependencies": {
    "@types/node": "^22.12.7",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  },
  "dependencies": {
    "@auth0/myaccount-js": "^1.0.0-beta.0",
    "@auth0/myorganization-js": "^1.0.0-beta.0",
    "zod": "^3.22.4"
  }
}
